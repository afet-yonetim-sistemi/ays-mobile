{
	"arrowParens": "always",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"singleQuote": true,
	"semi": true,
	"printWidth": 100,
	"tabWidth": 2,
	"trailingComma": "es5",
	"importOrder": ["^@/.*$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"useTabs": true,
	"endOfLine": "lf",
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
