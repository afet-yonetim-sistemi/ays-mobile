{
  "name": "ays-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "open-api": "node open-api-setter.js",
    "postinstall": "npm run open-api",
    "prepare": "husky install",
    "lint": "eslint . --fix --max-warnings=0",
    "format": "prettier . --write",
    "build:android": "npx eas build -p android",
    "android:pre": "env-cmd -f .env.pre npm run build:android -- --profile preview",
    "android:pre-local": "npm run android:pre -- --local",
    "android:prod": "env-cmd -f .env.production npm run build:android -- --profile production",
    "android:prod-local": "npm run android:prod -- --local"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": "eslint --cache --fix"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4.5.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.0.2",
    "axios": "^1.5.0",
    "axios-auth-refresh": "^3.3.6",
    "dotenv": "^16.3.1",
    "eslint-config-airbnb": "^19.0.4",
    "expo": "~49.0.11",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-localization": "~14.3.0",
    "expo-location": "~16.1.0",
    "expo-router": "^2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-task-manager": "~11.3.0",
    "expo-web-browser": "~12.3.2",
    "geolib": "^3.3.4",
    "i18next": "^23.5.1",
    "jotai": "^2.4.2",
    "jwt-decode": "^3.1.2",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.2",
    "react-i18next": "^13.2.2",
    "react-native": "0.72.6",
    "react-native-circular-progress-indicator": "^4.4.2",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-maps": "1.7.1",
    "react-native-maps-directions": "^1.9.0",
    "react-native-paper": "^5.10.6",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
  "private": true
}
